#include<iostream>
using namespace std;

struct items{
	int id;
	float cost;
	
};

int main(){
	items item[6];
	
	for (int I =0; i<4; i++){
		item[i].id = i+1;
		item[i].cost = (i+1)*25;
	}
	
	for (int I =0; i<4; i++){
		if (item[i].cost>50){
			cout<<"item id : "<<item[i].id<<"item cost : "<<item{i],cost<<endl;
		]
	}

	return 0;
}
